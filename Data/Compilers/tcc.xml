<?xml version="1.0" encoding="UTF-8" ?>
<compiler_data>
<macroargument>-D</macroargument>
<compiler>
	<name>tcc</name>
	
	<cmd>-I$INCLUDES -c $FLAGS $FILE -o ./$OBJDIRNAME/$FILE.o</cmd>
</compiler>
<linker>
	<dynamic_linker>
		<flags>
			<linux>-lrt -lpthread</linux>
		</flags>
		<name>tcc</name>
		<cmd>
			<win32>-shared $FLAGS $OBJECTS -o $OUTPUT.dll</win32>
			<linux>-shared $FLAGS $OBJECTS -o $OUTPUT.so</linux>
		</cmd>
	</dynamic_linker>
	<static_linker>
		<name>tiny_libmaker</name>
		<cmd>$OUTPUT.a $OBJECTS</cmd>
	</static_linker>
</linker>
</compiler_data>