<?xml version="1.0" encoding="UTF-8" ?>
<compiler_data>
<macroargument>-D</macroargument>
<compiler>
	<name>icl</name>
	<flags>-D_FILE_OFFSET_BITS=64</flags>
	<cmd>-I$INCLUDES /c $FLAGS $FILE /o ./$OBJDIRNAME/$FILE.o</cmd>
<linker>
	<dynamic_linker>
		<name>icl</name>
		<flags>-fPIC</flags>
		<cmd>
			<win32>/LD $FLAGS $OBJECTS -o $OUTPUT.dll</win32>
			<linux>-shared $FLAGS $OBJECTS -o $OUTPUT.so</linux>
		</cmd>
	</dynamic_linker>
	<static_linker>
		<name>ar</name>
		<cmd>rc $OUTPUT.a $OBJECTS</cmd>
	</static_linker>
</linker>
</compiler_data>