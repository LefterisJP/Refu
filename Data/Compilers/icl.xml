<?xml version="1.0" encoding="UTF-8" ?>
<compiler>
<flagsymbol>-D</flagsymbol>
<compile>icl -I$INCLUDES /c $FLAGS $FILE /o ./$OBJDIRNAME/$FILE.o</compile>
<link_dynamic_win32>icl /LD $FLAGS $OBJECTS -o $OUTPUT.dll</link_dynamic_win32>
<link_dynamic_linux>icl -shared $FLAGS $OBJECTS -o $OUTPUT.so</link_dynamic_linux>
<link_static>ar rc $OUTPUT.a $OBJECTS</link_static>
</compiler>